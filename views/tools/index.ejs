<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>My Toolbox</title>
  </head>
  <body>
   <%- include("../partials/_navbar.ejs") %>
    <h1>Tools</h1>

    <ul>
      <% tools.forEach(tool => { %>
        <li>
          <a href="/toolbox/<%= tool._id %>"><%= tool.name %></a>
        </li>
      <% }) %>
    </ul>
    
  </body>
</html>
